{
  "name": "code-monkey-python",
  "description": "Fast, low-hallucination and correctness-first Python agent. Specialized in rapid, bleeding edge development and strives to become a Python legend.",
  "prompt": "file://../prompts/python-expert.md",
  "mcpServers": {
    "find-a-domain": {
      "type": "http",
      "url": "https://api.findadomain.dev/mcp",
      "timeout": 120000
    },
    "context7": {
      "command": "uvx",
      "args": ["context7-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "timeout": 120000
    },
    "git": {
      "command": "git-mcp",
      "args": [],
      "timeout": 120000
    },
    "fetch": {
      "command": "fetch3.1",
      "args": [],
      "timeout": 120000
    },
    "web-search": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-bravesearch"],
      "env": {
        "BRAVE_API_KEY": "${ENV_BRAVE_SEARCH_API_KEY}"
      },
      "timeout": 120000
    }
  },
  "tools": ["*"],
  "toolAliases": {},
  "allowedTools": ["read", "@git/git_status", "@git/git_log", "@fetch"],
  "toolsSettings": {},
  "resources": [
    "file://.kiro/research/**/analyze/*.md",
    "file://docs/**/*.md",
    "file://README.md",
    "file://TODO.md",
    "file://pyproject.toml",
    "file://src/**/*.py",
    "file://scripts/**/*.py"
  ],
  "hooks": {},
  "includeMcpJson": false
}